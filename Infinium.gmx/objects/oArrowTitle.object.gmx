            <string>///Step

//Used for selection start or options
//if Pressing Up or Down arrow
if(keyboard_check_released(vk_down) || keyboard_check_released(vk_up)){
    //If not inposition
    if(y != pos1y){
        //set to position
        y = pos1y;
    //if not in postion 2
    } else if(y != pos2y){
        //set to position 2
        y = pos2y;
    }   
}

//Used for entering in command
//If pressing space or enter
if(keyboard_check_released(vk_space) || keyboard_check_released(vk_enter)){
    //if at pos1 == Start
    if(y = pos1y){
        //Go to Main room
        room_goto(rMain);
    //If in second pos == Options
    } else if(y = pos2y){
        //For now end, options room is not set up
        game_end();
    }   
}
</string>
